n  }\n\n  refer(acase){\n    acase.responsible_user = this.fulfill_form.get('refer_to').value;\n    acase.status = Status.INQUEUE;\n    acase.description = this.fulfill_form.get('description').value;\n    this.caseService.updateCase(acase).subscribe(() => this.getCases());\n  }\n  close(acase){\n    acase.status = Status.CLOSED;\n    acase.description = this.fulfill_form.get('description').value;\n    this.caseService.updateCase(acase).subscribe(() => this.getCases());\n  }\n  postpone(acase){\n    acase.status = Status.POSTPONED;\n    acase.description = this.fulfill_form.get('description').value;\n    this.caseService.updateCase(acase).subscribe(() => this.getCases());\n  }\n\n}\n","module.exports = \"body{\\r\\n    background-color: bisque;\\r\\n}\\r\\nh1{\\r\\n    text-align: center;\\r\\n}\\r\\nform label{\\r\\n    padding: 1vw;\\r\\n    display: block;\\r\\n    text-align: right;\\r\\n}\\r\\nform input, form select, form textarea{\\r\\n    float: left;\\r\\n    margin-left: 1vw;\\r\\n}\\r\\nform textarea{\\r\\n    width: 20vw;\\r\\n    height: 4vw\\r\\n}\\r\\nform button{\\r\\n    margin: 1vw 2vw;\\r\\n}\\r\\nform a{\\r\\n    margin: 0vw 1vw;\\r\\n    text-decoration: none;\\r\\n}\\r\\ntable, td, th {  \\r\\n    border: 1px solid #ddd;\\r\\n    text-align: center;\\r\\n}\\r\\ntable {\\r\\n    border-collapse: collapse;\\r\\n    width: 100%;\\r\\n    margin: 2vw 0vw;\\r\\n}\\r\\nth, td {\\r\\n    padding: 15px;\\r\\n}\\r\\ntd.button{\\r\\n    background-color: #ddd;\\r\\n    border: 2px solid #aaa;\\r\\n    color: #444;\\r\\n    text-shadow: none;\\r\\n}\\r\\ntd.button:hover{\\r\\n    cursor: pointer;\\r\\n}\\r\\nbutton{\\r\\n    color: #444;\\r\\n    padding: 5px 10px;\\r\\n    margin: 10px 2px;\\r\\n    background-color: #eee;\\r\\n    border-radius: 4px;\\r\\n}\\r\\nnav a.submenu, a.login_link {\\r\\n    padding: 5px 10px;\\r\\n    text-decoration: none;\\r\\n    margin: 10px 2px;\\r\\n    display: inline-block;\\r\\n    background-color: #eee;\\r\\n    bord