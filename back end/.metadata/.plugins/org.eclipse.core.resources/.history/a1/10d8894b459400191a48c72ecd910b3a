username_is_duplicate = false;\n  required_error_message = \"*\";\n  roles : string[] = [];\n  user : User = new User();\n  edit_profile_form = new FormGroup({\n    firstname : new FormControl('', Validators.required),\n    lastname : new FormControl('', Validators.required),\n    username : new FormControl({value:'', disable:true}, Validators.required),\n    role : new FormControl({value:'', disabled:true}, Validators.required),\n    confirmed : new FormControl({value: '', disabled: true}, Validators.required),\n    password : new FormControl('', [Validators.required, Validators.minLength(8)]),\n    confirm_password : new FormControl('', Validators.required),\n  })\n\n  get firstname() { return this.edit_profile_form.get('firstname'); }\n  get lastname() { return this.edit_profile_form.get('lastname'); }\n  get username() { return this.edit_profile_form.get('username'); }\n  get role() { return this.edit_profile_form.get('role'); }\n  get confirmed() {return this.edit_profile_form.get('confirmed')};\n  get password() { return this.edit_profile_form.get('password'); }\n  get confirm_password() { return this.edit_profile_form.get('confirm_password'); }\n\n  constructor(\n    private authService : AuthService,\n    private userService : UserService,\n    public router: Router,\n  ) { }\n\n  ngOnInit() {\n    this.userService.getUserByUsername().subscribe(answer=>{\n      if(answer.success){\n        this.set_form_values(answer.data);\n      }\n    })\n    for(let key in Roles){\n      this.roles.push(Roles[key]);\n    }\n  }\n\n  set_form_values(user:User){\n    this.user = user;\n    this.edit_profile_form.setValue({\n      firstname : user.firstname,\n      lastname : user.lastname,\n      username : user.username,\n      role : user.role,\n      confirmed : user.confirmed,\n      password : user.password,\n      confirm_password : user.password,\n    })\n  }\n  edit(){\n    let editUser : User = {\n      firstname : this.firstname.value,\n      lastname : this.lastname.value,\n      username : this.username.value,\n      password : this.password.value,\n      role : this.role.value || Roles.STUDENT,\n      confirmed : this.confirmed.value,\n      token : localStorage.getItem('token'),\n    };\n    if (this.password.value != this.confirm_password.value){\n      return;\n    }\n    this.userService.updateUser(editUser).subscribe(answer=>{\n      alert(answer.message)\n      if(answer.success){\n        let redirect = \"/student\";\n        this.router.navigateByUrl(redirect);\n      }\n    })\n  }\n\n  not_changed():boolean{\n    let flag = true;\n    if (this.edit_profile_form.get('firstname').value != this.user.firstname){flag = false;}\n    if (this.edit_profile_form.get('lastname').value != this.user.lastname){flag = false;}\n    if (this.edit_profile_form.get('role').value != this.user.role){flag = false;}\n    if (this.edit_profile_form.get('password').value != this.user.password){flag = false;}\n    return flag;\n  }\n\n  checkUsername(input : any):void{\n    if (input.value == \"\" || input.value == this.user.username){\n      this.username_is_duplicate = false;\n      input.style.backgroundColor=\"white\";\n      return;\n    }\n    //input.style.backgroundColor=\"green\";\n    let value : string = input.value;\n    this.authService.is_new_username(value).subscribe(answer =>{\n      if(answer.data)\n      {\n        input.style.backgroundColor= Colors.GREEN.TEXTBOX ;\n        this.username_is_duplicate = false;\n      }\n      else{\n        input.style.backgroundColor=\"red\";\n        this.username_is_duplicate = true;\n      }\n    });\n  }\n\n  checkPassword(passwordinput:any, cpasswordinput:any):boolean{\n    if(passwordinput.value == cpasswordinput.value){\n      return true;\n    }\n    else{\n      return false;\n    }\n  }\n\n  passwordChanged():boolean{\n    if (this.edit_profile_form.get('password').value != this.user.password){return true;}\n    else{return false;}\n  }\n\n}\n","module.exports = \"body{\\r\